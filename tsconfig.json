{
  "compilerOptions": {
    "allowJs": false,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2019"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "esnext",
    "types": ["react", "react-native", "jest", "node"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["App/*"]
    },
  },
  "include": [
    "App/**/*",
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
  ]
  
}
